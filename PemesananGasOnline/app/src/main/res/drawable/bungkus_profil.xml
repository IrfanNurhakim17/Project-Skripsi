<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="90dp"
        android:width="90dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <gradient
                android:startColor="#00B9A3"
                android:endColor="#007253"
                android:angle="270"/>
            <corners
                android:radius="100dp">
            </corners>
        </shape>
    </item>
    <item
        android:width="72dp"
        android:height="72dp"
        android:gravity="center"
        android:drawable="@drawable/ic_profil">
    </item>

</layer-list>